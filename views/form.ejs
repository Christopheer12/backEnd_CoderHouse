<h3 class="text-primary">Ingrese Datos</h3>
<br />
<form action="/" method="post">
  <div class="form-group">
    <label for="title" class="font-weight-bold">Nombre</label
    ><input type="text" name="nombre" class="form-control" />
  </div>
  <div class="form-group">
    <label for="price" class="font-weight-bold">Precio</label
    ><input type="number" name="apellido" class="form-control" />
  </div>
  <div class="form-group">
    <label for="imagen" class="font-weight-bold">Foto</label
    ><input type="text" name="edad" class="form-control" />
  </div>
  <input type="submit" value="Enviar" class="btn btn-success mt-3 mb-5" />
</form>
